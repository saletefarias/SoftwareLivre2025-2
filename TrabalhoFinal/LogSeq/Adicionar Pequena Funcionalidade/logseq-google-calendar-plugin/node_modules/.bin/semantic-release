#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/semantic-release@21.1.2_typescript@4.9.5/node_modules/semantic-release/bin/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/semantic-release@21.1.2_typescript@4.9.5/node_modules/semantic-release/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/semantic-release@21.1.2_typescript@4.9.5/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/semantic-release@21.1.2_typescript@4.9.5/node_modules/semantic-release/bin/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/semantic-release@21.1.2_typescript@4.9.5/node_modules/semantic-release/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/semantic-release@21.1.2_typescript@4.9.5/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semantic-release/bin/semantic-release.js" "$@"
else
  exec node  "$basedir/../semantic-release/bin/semantic-release.js" "$@"
fi
