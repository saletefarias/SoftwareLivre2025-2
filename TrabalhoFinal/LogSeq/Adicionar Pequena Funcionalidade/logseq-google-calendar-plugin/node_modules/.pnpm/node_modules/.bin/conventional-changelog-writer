#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/conventional-changelog-writer@6.0.1/node_modules/conventional-changelog-writer/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/conventional-changelog-writer@6.0.1/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/conventional-changelog-writer@6.0.1/node_modules/conventional-changelog-writer/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/conventional-changelog-writer@6.0.1/node_modules:/home/manoel/Projetos/logseq-google-calendar-plugin/logseq-plugin-template-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-changelog-writer/cli.js" "$@"
else
  exec node  "$basedir/../conventional-changelog-writer/cli.js" "$@"
fi
